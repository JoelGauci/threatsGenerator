<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<XMLThreatProtection async="false" continueOnError="false" enabled="true" name="XML-Threat-Protection-Test">
    <DisplayName>XML Threat Protection-Test</DisplayName>
    <Properties/>
    <NameLimits>
        <Element>20</Element>
        <Attribute>20</Attribute>
        <NamespacePrefix>10</NamespacePrefix>
        <ProcessingInstructionTarget>10</ProcessingInstructionTarget>
    </NameLimits>
